<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="EditorSettings">
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_PERSISTENT" value="true" />
  </component>
  <component name="LiveEditOptions" chromeUpdateOnChanges="false" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="ff392f" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="3aaaff" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
  </component>
</application>